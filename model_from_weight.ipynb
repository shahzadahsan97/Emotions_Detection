    "from vgg import VGG\n",
    "import os\n",
    "\n",
    "model = VGG()\n",
    "print(model.summary())\n",
    "weights = './Xml_File/emotion_best_weights.h5'\n",
    "if os.path.exists(weights):\n",
    "    print(\"Loading weight\")\n",
    "    model.load_weights(weights)\n",
    "    print(\"Saving model\")\n",
    "    model.save(\"./Xml_File/emotion_recognition.h5\")\n"
   ]
  }
